		"framer-motion": "^2.9.4",
		"gatsby": "^2.24.61",
		"gatsby-image": "^2.4.19",
		"gatsby-plugin-emotion": "^5.0.0",
		"gatsby-plugin-manifest": "^2.4.30",
		"gatsby-plugin-netlify-cache": "^1.2.0",
		"gatsby-plugin-react-helmet": "^3.3.11",
		"gatsby-plugin-robots-txt": "^1.5.3",
