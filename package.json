{
	"name": "mastrero",
	"description": "Repository for my portfolio. Built with Gatsby",
	"private": true,
	"version": "0.1.0",
	"license": "0BSD",
	"keywords": [
		"mastrero",
		"ArunKumar Nadikattu",
		"gatsby"
	],
	"scripts": {
		"develop": "gatsby develop",
